# vim:ft=sshconfig:

ServerAliveInterval 60
Compression yes
ForwardAgent yes

# Share connections
ControlMaster auto
ControlPath /tmp/ssh_%r@%h:%p
ControlPersist yes

Host tunnel-local
    HostName 0.0.0.0
    LocalForward 80 127.0.0.1:8080

Host rh-dev
    HostName ceehadoop1.gsslab.rdu2.redhat.com

Host rh-docker
    HostName vm78.gsslab.rdu2.redhat.com
    User telemetry

Host rh-pek
    HostName 10.66.209.103
    User root
    ForwardX11 yes

Host *.redhat.com rh-*
    User ngao

Host server
    HostName ngao-server.usersys.redhat.com

Host work
    HostName 10.66.192.22
