# vim:ft=sshconfig:

ServerAliveInterval 60
Compression yes
ForwardAgent yes

# Share connections
ControlMaster auto
ControlPath /tmp/ssh_%r@%h:%p
ControlPersist yes

Host tunnel-local
    HostName 0.0.0.0
    LocalForward 80 127.0.0.1:8080

Host ceehadoop?
    HostName %h.gsslab.rdu2.redhat.com
    User ngao

Host atomic
    HostName vm78.gsslab.rdu2.redhat.com
    User telemetry

Host work
    HostName 10.66.192.135
